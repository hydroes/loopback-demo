{
  "restApiRoot": "/api",
  "host": "0.0.0.0",
  "port": 3001,
  "serviceDiscovery": {
    "registerService": true,
    "uniqueServiceName": "microservice-a",
    "healthcheckUrl": "http://localhost:3001/api/consuls/healthcheck",
    "interval": "220ms",
    "tags": ["ob-microservices"],
    "deregistercriticalserviceafter": "1m",
    "timeout": "5m"
  },
  "remoting": {
    "context": false,
    "rest": {
      "handleErrors": false,
      "normalizeHttpPath": false,
      "xml": false
    },
    "json": {
      "strict": false,
      "limit": "100kb"
    },
    "urlencoded": {
      "extended": true,
      "limit": "100kb"
    },
    "cors": false
  }
}
